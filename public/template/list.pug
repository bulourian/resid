include head.pug
div(class="container text-right")
  table(class="table table-hover")
    thead
      tr
        th(scope="col") #
        th(scope="col") نوع
        th(scope="col") شماره بیمه
        th(scope="col") نام و نام خانوادگی
        th(scope="col") شماره قسط
        th(scope="col") مبلغ
        th(scope="col") تاریخ
        th(scope="col") بیشتر
    tbody
    each va in val
      tr
        th(scope)= va.id
        td= va.type
        td= va.bid
        td= va.name
        td= va.reciept
        td= va.payment
        td= va.date
        td amin
        
